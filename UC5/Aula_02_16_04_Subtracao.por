programa
{
	
	funcao inicio()
	{
		inteiro n1, n2, resultado

		escreva(" LÓGICA COM SUBTRAÇÃO ")
		
		escreva("\n")
		escreva("\n")
		
		escreva("Digite um número para que seja subtraido: ")
			leia(n1)

		escreva("\n")
	
		escreva("Digite outro número para que seja subtraido: ")
			leia(n2)

		resultado = n1 - n2

		escreva("\n")

		escreva("--------------------------- RESULTADO ---------------------------")
		escreva("\n")
		escreva("\n")
		escreva("\n")

		escreva("O resultado dos números subtraidos é: ", resultado)
	}
}
