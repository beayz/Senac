programa
{
	
	funcao inicio()
	{
		inteiro n1

		n1 = 15

		enquanto (n1 <= 200){
			escreva("O quadrado de ", n1, " é : ", n1 * n1, "\n")
			n1 = n1 + 1
		}
		
	}
}
